<Window x:Class="Timon.Exe.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:BotEngine.UI="clr-namespace:BotEngine.UI;assembly=BotEngine.UI"
        xmlns:BotSharp.UI.Wpf="clr-namespace:BotSharp.UI.Wpf;assembly=BotSharp.UI"
        xmlns:Timon.UI="clr-namespace:Timon.UI;assembly=Timon.UI"
        xmlns:local="clr-namespace:Timon.Exe"
        mc:Ignorable="d"
        Title="Timon" Height="444" Width="525">
    <Grid>
        <TabControl>
            <TabItem Header="Script">
                <BotSharp.UI.Wpf:IDE
                    Name="ScriptIDE"></BotSharp.UI.Wpf:IDE>
            </TabItem>
            <TabItem Header="Config">
                <Timon.UI:ConfigView Name="ConfigView"></Timon.UI:ConfigView>
            </TabItem>
        </TabControl>
    </Grid>
</Window>
